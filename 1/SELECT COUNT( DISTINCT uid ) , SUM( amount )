SELECT COUNT( DISTINCT uid ) , SUM( amount ) 
FROM  `payment_201712` 
WHERE uid
IN (

SELECT uid
FROM  `platform` 
WHERE appid =203
AND pid LIKE  '0086%'
AND TIME >=1511884800 +7 *86400
AND TIME <1511971200 +7 *86400
)
AND createtime >=1511884800 +7 *86400
AND createtime <1511971200 +7 *86400
AND state =2

